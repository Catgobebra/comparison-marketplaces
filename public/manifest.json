{
  "short_name": "React App",
  "name": "cute futanari",
  "version": "1.0.0.0",
  "manifest_version": 3,
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "storage",
    "http://localhost:5018/*"
  ],
  "host_permissions": [
    "https://www.ozon.ru/*",
    "http://localhost:5018/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}